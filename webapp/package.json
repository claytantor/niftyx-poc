    "parcel-reporter-static-files-copy": "^1.4.0",
    "postcss-custom-properties": "^12.1.11",
    "postcss-url": "^10.1.3",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
