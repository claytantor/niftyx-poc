{
  "name": "niftyx-web",
  "version": "1.0.0",
  "description": "",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel --host 0.0.0.0 -p 3010 src/index.html --no-cache",
    "serve-local": "bash ./scripts/create-deployment-json.sh local && cp ./environment/deployment.json ./src/. && parcel build && parcel --host 0.0.0.0 -p 3010 src/index.html --no-cache",
    "deploy-dev": "cp src/assets/favicon-32x32.png dist/favicon.ico && bash ./scripts/create-deployment-json.sh dev && cp ./environment/deployment.json ./src/. && npm run build && bash ./scripts/deploy.sh dev",
    "deploy-prd": "cp src/assets/favicon-32x32.png dist/favicon.ico && bash ./scripts/create-deployment-json.sh prd && cp ./environment/deployment.json ./src/. && npm run build && bash ./scripts/deploy.sh prd",
    "build": "parcel build"
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "buffer": "^5.7.1",
